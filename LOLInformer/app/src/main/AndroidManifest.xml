<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.artlite.collapsinglayouttest">

    <application
        android:name=".core.application.CurrentApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_reneval"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.activities.MainActivity"
            android:configChanges="orientation|keyboard"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.activities.ChampionDetailActivity"
            android:configChanges="orientation|keyboard"
            android:label="@string/text_champion_details" />
    </application>

</manifest>
